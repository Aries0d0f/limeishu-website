<jumbled-order-exercise><main-order><segment>It</segment>
<segment>was</segment>
<segment>a</segment>
<segment>dream</segment>
<segment>shared</segment>
<segment>by few</segment>
<segment>.</segment>
</main-order></jumbled-order-exercise>
